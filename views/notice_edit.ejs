        <section>
            <h2>공지사항 작성하기</h2>
            <form method="post" action="/store">
            <table>
                <tr>
                    <input type="hidden" name="id" value="<%=row.id%>">
                    <td><label>제목</label></td>
                    <td class="ti">
                        <input type="text" name="content" id="content" value="<%=row.content%>">
                        <span>(※ 최대 10자 이내)</span>
                    </td>

                </tr>

                <tr>                    
                    <td id="content">내용 작성 : </td>
                    <td class="con">
                        <textarea name="inner_content"><%=row.inner_content%></textarea>
                        <span>(※ 최대 50자 이하)</span>
                    </td>
                </tr>
                <tr>
                    <td  colspan="2">
                        <input type="submit" value="수정" id="save">
                        <input type="button" value="뒤로 가기" onclick="javascript:location.href='/notice_content?id=<%=row.id%>'">
                    </td>
                </tr>
            </table>
        </form>
        </section>